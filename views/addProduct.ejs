<section class="vh-100" style="background-color: #eee;">
    <div class="container h-100">
        <div class="row d-flex justify-content-center align-items-center h-100">
            <div class="col-lg-12 col-xl-11">
                <div class="card text-black" style="border-radius: 25px;">
                    <div class="card-body p-md-5">
                        <div class="row justify-content-center">
                            <div class="col-md-10 col-lg-6 col-xl-5 order-2 order-lg-1">


                                <h1>Add new product</h1>
                                <p>Please fill the details below to add new product</p>
                                <hr>

                                <form class="mx-1 mx-md-4">
                                    <!-- product name name -->
                                    <div class="d-flex flex-row align-items-center mb-4">
                                        <i class="fas fa-user fa-lg me-3 fa-fw"></i>
                                        <div class="form-outline flex-fill mb-0">
                                            <input type="text" id="productName" placeholder="Enter Product Name"
                                                class="form-control" />
                                            <label class="form-label" for="productName">Product Name</label>
                                        </div>
                                    </div>
                                    <!-- Product image -->
                                    <div class="d-flex flex-row align-items-center mb-4">
                                        <i class="fas fa-user fa-lg me-3 fa-fw"></i>
                                        <div class="form-outline flex-fill mb-0">
                                            <label>Select Image File:</label>
                                            <input type="file" name="image">
                                            <input type="submit" name="submit" value="Upload">
                                        </div>
                                    </div>
                                    <!-- brand -->
                                    <div class="d-flex flex-row align-items-center mb-4">
                                        <i class="fas fa-user fa-lg me-3 fa-fw"></i>
                                        <div class="form-outline flex-fill mb-0">
                                            <input type="text" id="brand" placeholder="Brand" class="form-control" />
                                            <label class="form-label" for="brand">Brand</label>
                                        </div>
                                    </div>
                                    <!-- Category -->
                                    <div class="d-flex flex-row align-items-center mb-4">
                                        <i class="fas fa-user fa-lg me-3 fa-fw"></i>
                                        <div class="form-outline flex-fill mb-0">
                                            <input type="text" id="category" placeholder="Category"
                                                class="form-control" />
                                            <label class="form-label" for="category">Category</label>
                                        </div>
                                    </div>
                                    <!-- description -->
                                    <div class="d-flex flex-row align-items-center mb-4">
                                        <i class="fas fa-user fa-lg me-3 fa-fw"></i>
                                        <div class="form-outline flex-fill mb-0">
                                            <input type="text" id="description" placeholder="Descripe Product"
                                                class="form-control" />
                                            <label class="form-label" for="Description"></label>
                                        </div>
                                    </div>
                                    <!-- price -->
                                    <div class="d-flex flex-row align-items-center mb-4">
                                        <i class="fas fa-user fa-lg me-3 fa-fw"></i>
                                        <div class="form-outline flex-fill mb-0">
                                            <input type="number" id="price" placeholder="Price" class="form-control" />
                                            <label class="form-label" for="price">Price</label>
                                        </div>
                                    </div>
                                    <!-- countInStock -->
                                    <div class="d-flex flex-row align-items-center mb-4">
                                        <i class="fas fa-user fa-lg me-3 fa-fw"></i>
                                        <div class="form-outline flex-fill mb-0">
                                            <input type="number" id="countInStock" placeholder="Count In Stock"
                                                class="form-control" />
                                            <label class="form-label" for="countInStock">Count In Stock</label>
                                        </div>
                                    </div>
                                    <!-- rating -->
                                    <div class="d-flex flex-row align-items-center mb-4">
                                        <i class="fas fa-user fa-lg me-3 fa-fw"></i>
                                        <div class="form-outline flex-fill mb-0">
                                            <input type="number" id="rating" placeholder="Rating"
                                                class="form-control" />
                                            <label class="form-label" for="rating">Rating</label>
                                        </div>
                                    </div>
                                    <!-- numReviews -->
                                    <div class="d-flex flex-row align-items-center mb-4">
                                        <i class="fas fa-user fa-lg me-3 fa-fw"></i>
                                        <div class="form-outline flex-fill mb-0">
                                            <input type="number" id="numReviews" placeholder="Rating"
                                                class="form-control" />
                                            <label class="form-label" for="numReviews">Reviews</label>
                                        </div>
                                    </div>

                            </div>

                            </form>

                        </div>
                    </div>
                    <div class="col-md-10 col-lg-6 col-xl-7 d-flex align-items-center order-1 order-lg-2">

